config_version=5

[application]

config/name="HAT_Game"
run/main_scene="res://scenes/ui/MainMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="HAT_Game"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameEvents="*res://scenes/autoload/GameEvents.tscn"
LevelManager="*res://scenes/autoload/LevelManager.tscn"
SaveManager="*res://scenes/autoload/SaveManager.tscn"
SettingManager="*res://scenes/autoload/SettingManager.tscn"
Cursor="*res://scenes/autoload/Cursor.tscn"
AudioHelpers="*res://scenes/autoload/AudioHelpers.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1920
window/size/window_height_override=1080

[dotnet]

project/assembly_name="HAT_Game"

[editor]

naming/scene_name_casing=1
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[gui]

theme/custom="res://resources/sci-fi-theme.tres"

